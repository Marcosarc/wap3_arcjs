{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "CHROME_AWS_LAMBDA_CHROMIUM_EXECUTABLE_PATH": "/tmp/chromium",
    "PUPPETEER_EXECUTABLE_PATH": "/tmp/chromium"
  }
}